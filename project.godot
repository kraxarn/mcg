; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="MobileCardGames"
config/description="Various card/board games primarily for Mobile devices, with online multiplayer."
run/main_scene="res://Scenes/Dev/Menu.tscn"
boot_splash/image="res://Asset/Image/Splash.png"
boot_splash/fullsize=false
boot_splash/bg_color=Color( 0.2, 0.6, 0.854902, 1 )
config/icon="res://res/launcher.png"
config/macos_native_icon="res://res/macos/icon.icns"
config/windows_native_icon="res://res/windows/icon.ico"

[display]

window/size/width=540
window/size/height=960
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="expand"

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_clear_color=Color( 0.2, 0.6, 0.854902, 1 )
environment/default_environment="res://default_env.tres"
