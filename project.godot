; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Mobile Card Games"
config/description="Card games, and other board games, primarily for mobile devices"
run/main_scene="res://scenes/developer/menu.tscn"
config/features=PackedStringArray("4.0", "Mobile")
boot_splash/image="res://assets/images/splash.png"
boot_splash/fullsize=false
config/icon="res://res/launcher.png"
config/macos_native_icon="res://res/macos/icon.icns"
config/windows_native_icon="res://res/windows/icon.ico"

[display]

window/size/viewport_width=540
window/size/viewport_height=960
window/handheld/orientation=1

[dotnet]

project/assembly_name="mcg"

[rendering]

renderer/rendering_method="mobile"
